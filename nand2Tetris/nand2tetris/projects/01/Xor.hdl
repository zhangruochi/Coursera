/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a,out=NotA);
    Not(in=b,out=NotB);
    And(a=a,b=NotB,out=aAndNotB);
    And(a=NotA,b=b,out=bAandNotA);
    Or(a=aAndNotB,b=bAandNotA,out=out);
}